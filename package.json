{
  "name": "llmdesktopmascot",
  "productName": "llmdesktopmascot",
  "version": "1.0.0",
  "description": "My Electron application description",
  "type": "module",
  "main": ".vite/build/main.cjs",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:react": "eslint src/renderer/**/*.{ts,tsx}",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:react": "prettier --write src/renderer/**/*.{ts,tsx}",
    "type-check": "tsc --noEmit",
    "type-check:react": "tsc --noEmit --project tsconfig.json",
    "quality": "npm run type-check && npm run lint && npm run format:check",
    "quality:fix": "npm run type-check && npm run lint:fix && npm run format",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test --config=playwright-e2e.config.ts",
    "test:e2e:ui": "playwright test --config=playwright-e2e.config.ts --ui",
    "test:e2e:debug": "playwright test --config=playwright-e2e.config.ts --debug",
    "test:e2e:xvfb": "./scripts/run-e2e-xvfb.sh",
    "test:e2e:wsl": "./scripts/run-e2e-xvfb.sh --grep 'WSL ヘッドレスE2Eテスト'",
    "test:e2e:headless": "playwright test --config=playwright-e2e.config.ts tests/e2e/wsl/headless-basic.spec.ts",
    "test:coverage": "vitest run --coverage",
    "test:ci": "npm run quality && npm run test && npm run test:e2e",
    "test:performance": "npm run test:perf:startup && npm run test:perf:memory && npm run test:perf:e2e && npm run test:perf:integration",
    "test:perf:startup": "vitest run src/tests/performance/startupOptimization.test.ts --reporter=json --outputFile=test-results/performance/startup-results.json",
    "test:perf:memory": "vitest run src/tests/performance/memoryUsage.test.ts --reporter=json --outputFile=test-results/performance/memory-results.json",
    "test:perf:e2e": "npm run test:e2e:headless -- --reporter=json --output-dir=test-results/e2e-performance",
    "test:perf:integration": "npm run test:e2e:headless -- tests/e2e/integration/comprehensive-integration.spec.ts --reporter=json --output-dir=test-results/integration-performance",
    "test:perf:ci": "npm run test:performance && node scripts/analyze-performance.js",
    "test:accessibility": "npm run test:e2e:headless -- tests/e2e/accessibility/accessibility-compliance.spec.ts --reporter=json --output-dir=test-results/accessibility",
    "test:accessibility:audit": "npm run test:accessibility && node scripts/accessibility-audit.js",
    "test:cross-platform": "npm run test:cross-platform:current",
    "test:cross-platform:current": "node scripts/cross-platform-validator.js",
    "test:cross-platform:linux": "node scripts/cross-platform-validator.js --platform=linux",
    "test:cross-platform:windows": "node scripts/cross-platform-validator.js --platform=windows",
    "test:cross-platform:macos": "node scripts/cross-platform-validator.js --platform=macos",
    "analyze:bundle": "vite-bundle-analyzer",
    "analyze:size": "npm run package && node -e \"console.log(require('./src/tests/performance/bundleSize.test.ts').BundleAnalyzer.analyzeBundleComposition('.vite/build'))\"",
    "analyze:performance": "node scripts/analyze-performance.js",
    "analyze:accessibility": "node scripts/accessibility-audit.js",
    "analyze:cross-platform": "node scripts/cross-platform-validator.js",
    "build:analyze": "vite build --mode=analyze",
    "perf:bundle": "vitest run src/tests/performance/bundleSize.test.ts"
  },
  "keywords": [],
  "author": {
    "name": "KB",
    "email": "KotsBeirne@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@playwright/experimental-ct-react": "^1.52.0",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4.1.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/glob": "^8.1.0",
    "@types/node": "^22.15.28",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/three": "^0.176.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@vitejs/plugin-react": "^4.5.0",
    "autoprefixer": "^10.4.21",
    "electron": "36.3.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vitest": "^0.5.4",
    "glob": "^11.0.2",
    "jsdom": "^26.1.0",
    "playwright-electron": "^0.5.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "rollup-plugin-analyzer": "^4.0.0",
    "tailwindcss": "^4.1.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-bundle-analyzer": "^0.22.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^5.0.1",
    "@modelcontextprotocol/server-puppeteer": "^2025.5.12",
    "@modelcontextprotocol/server-sequential-thinking": "^0.6.2",
    "@pixiv/three-vrm": "^3.4.1",
    "@pixiv/three-vrm-animation": "^3.4.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.1",
    "immer": "^10.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "tailwind-merge": "^3.3.0",
    "three": "^0.176.0",
    "zod": "^3.25.46",
    "zustand": "^5.0.5"
  }
}
